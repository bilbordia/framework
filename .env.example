VITE_GA_MEASUREMENT_ID= ## your Google Analytics ID (e.g., G-XXXXXXXXXX)

## Customer Support
VITE_CHATWOOT_BASE_URL= ## e.g., https://app.chatwoot.com
VITE_CHATWOOT_WEBSITE_TOKEN= ## Your Chatwoot Website Inbox Token

## Email Marketing
EMAIL_MARKETING_PROVIDER="none" # Options: "kit", "none"
EMAIL_MARKETING_BASE_URL="https://api.convertkit.com" # Base URL for Kit API (v3)
EMAIL_MARKETING_API_KEY="" # Required for Kit
EMAIL_MARKETING_TAG_ID="" # Required for Kit (Tag to add users to)
EMAIL_MARKETING_CUSTOM_USER_ID_FIELD="" # Required for Kit (e.g., "cf_supabase_user_id")
EMAIL_MARKETING_CUSTOM_CREATED_AT_FIELD="" # Required for Kit (e.g., "cf_supabase_created_at")

